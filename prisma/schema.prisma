generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model Course {
  id                          String   @id @default(cuid())
  career                      String
  code                        String
  college                     String
  courseNumber                String
  creditsAcademicProgress     Float?
  description                 String?
  effectiveStartDate          DateTime?
  effectiveEndDate            DateTime?
  longName                    String
  name                        String
  status                      String
  subjectCode                 String
}

model Professor {
  id        String   @id @default(cuid())
  name      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}